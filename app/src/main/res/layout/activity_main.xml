<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.opet.diogo.instagramclone.MainActivity">

    <Button
        android:id="@+id/btn_gotoUpload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/subir_foto"
        android:onClick="gotoUpload"/>

    <Space
        android:layout_width="match_parent"
        android:layout_height="20sp" />

    <Button
        android:id="@+id/btn_gotoSeeAll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/ver_fotos"
        android:onClick="gotoSeeAll"/>

</LinearLayout>
